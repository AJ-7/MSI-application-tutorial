        <script src="/example/dist/inline.bundle.js" defer></script>
		<script src="/example/dist/vendor.bundle.js" defer></script>
		<script src="/example/dist/main.bundle.js" defer></script>
	<% } %>

    <script>
		document.locale = 'en';
		
		<%if (envrionment === 'development') { %>
		System.import('app').catch(function(err){ console.error(err); });
		<% } %>
    </script>
    
	<input type="hidden" id="env" value="<%= envrionment %>" />
	
